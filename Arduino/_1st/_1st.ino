void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(0,HIGH);
  delay(500);
  
  digitalWrite(0,LOW);
  digitalWrite(1,HIGH);
  delay(500);
  
  digitalWrite(1,LOW);
  digitalWrite(2,HIGH);
  delay(500);

  digitalWrite(2,LOW);
  digitalWrite(3,HIGH);
  delay(500);

  digitalWrite(3,LOW);
  digitalWrite(4,HIGH);
  delay(500);

  digitalWrite(4,LOW);
  digitalWrite(5,HIGH);
  delay(500);

  digitalWrite(5,LOW);
    
}
